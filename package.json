    "if-env": "^1.0.4",
    "js-file-download": "^0.4.4",
    "mysql2": "^1.6.4",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "passport-local-sequelize": "^0.8.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
